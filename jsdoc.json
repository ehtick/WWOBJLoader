{
  "source": {
    "include": [
      "src/loaders/OBJLoader2.js",
      "src/loaders/OBJLoader2Parallel.js",
      "src/loaders/utils/MtlObjBridge.js",
      "src/loaders/utils/MaterialStore.js",
      "src/loaders/utils/MaterialUtils.js",
      "src/loaders/utils/TransportUtils.js",
      "src/loaders/workerTaskManager/WorkerTaskManager.js",
      "src/loaders/workerTaskManager/worker/defaultRouting.js",
      "src/loaders/workerTaskManager/worker/tmOBJLoader2.js"
    ]
  },
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./build/docs"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
